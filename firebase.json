{
  "firestore": {
    "database": "(default)",
    "location": "asia-south2",
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "Public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/professor",
        "destination": "/professor.html"
      },
      {
        "source": "/classroom",
        "destination": "/classroom.html"
      },
      {
        "source": "/academic",
        "destination": "/academic.html"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "index",
        "destination": "/index.html"
      },
      {
        "source": "/academic-calendar",
        "destination": "/academic-calendar.html"
      },
        {
        "source": "/about",
        "destination": "/about.html"
      },
         {
        "source": "/hotspot",
        "destination": "/hotspot.html"
      },
         {
        "source": "/contact",
        "destination": "/contact.html"
      }
    ]
  }
}
